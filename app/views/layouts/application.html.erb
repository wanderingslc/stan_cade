  </head>

  <body>
  <%= render 'layouts/navbar' %>
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value %></div>
  <% end %>
  <div class="container">

    <%= yield %>
  </div>
  </body>
</html>
